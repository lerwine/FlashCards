<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.erwinefamily.net/FlashCards/Deck" 
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:fcd="http://www.erwinefamily.net/FlashCards/Deck">
  <xs:element name="Deck">
    <xs:complexType>
      <xs:all>
        <xs:element name="Categories" type="fcd:CardCategoryCollectionType">
          <xs:unique name="CategoryName">
            <xs:selector xpath="fcd:Category" />
            <xs:field xpath="@Name" />
          </xs:unique>
        </xs:element>
        <xs:element name="Cards" type="fcd:CardCollectionType">
          <xs:keyref name="CardCategory" refer="fcd:CategoryKey">
            <xs:selector xpath="fcd:Card" />
            <xs:field xpath="@CategoryID" />
          </xs:keyref>
        </xs:element>
      </xs:all>
      <xs:attribute name="ID" type="fcd:GuidType" use="required" />
      <xs:attribute name="Title" type="fcd:NonEmptySTringType" use="required" />
      <xs:attribute name="ForeColor" type="fcd:Color" use="required" />
      <xs:attribute name="BorderColor" type="fcd:Color" use="required" />
      <xs:attribute name="BackColor1" type="fcd:Color" use="required" />
      <xs:attribute name="BackColor2" type="fcd:Color" use="required" />
      <xs:attribute name="FillType" type="fcd:FillType" use="required" />
    </xs:complexType>
    <xs:key name="CategoryKey">
      <xs:selector xpath="fcd:Categories/fcd:Category" />
      <xs:field xpath="@ID" />
    </xs:key>
  </xs:element>

  <xs:complexType name="CardCategoryCollectionType">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Category" type="fcd:CardCategoryType" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="CardCategoryType">
    <xs:attribute name="ID" type="fcd:GuidType" use="required" />
    <xs:attribute name="Name" type="fcd:NonEmptySTringType" use="required" />
  </xs:complexType>

  <xs:complexType name="CardCollectionType">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Card" type="fcd:CardType" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CardType">
    <xs:choice minOccurs="1" maxOccurs="1">
      <xs:element name="NumberProblem" type="fcd:NumberCardType" />
      <xs:element name="FloatingPointProblem" type="fcd:FloatingPointCardType" />
      <xs:element name="FractionProblem" type="fcd:FractionCardType" />
      <xs:element name="MultipleChoice" type="fcd:MultipleChoiceCardType" />
      <xs:element name="QandA" type="fcd:QandACardType" />
    </xs:choice>
    <xs:attribute name="ID" type="fcd:GuidType" use="required" />
    <xs:attribute name="CategoryID" type="fcd:GuidType" use="optional" />
  </xs:complexType>

  <xs:complexType name="FractionCardType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="NumeratorAnswer" type="xs:integer" use="required" />
        <xs:attribute name="DenominatorAnswer" type="xs:integer" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="FloatingPointCardType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="Answer" type="xs:double" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="NumberCardType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="NumberAnswer" type="xs:long" use="required" />
        <xs:attribute name="RemainderAnswer" type="xs:long" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="MultipleChoiceCardType">
    <xs:all>
      <xs:element name="Question" type="xs:string" />
      <xs:element name="Choices">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="2" maxOccurs="unbounded" name="Choice">
              <xs:complexType>
                <xs:all>
                  <xs:element name="Value" type="xs:string" />
                  <xs:element name="Explanation" type="xs:string" minOccurs="0" />
                </xs:all>
                <xs:attribute name="Correct" type="xs:boolean" use="required" />
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="QandACardType">
    <xs:all>
      <xs:element name="Question" type="xs:string" />
      <xs:element name="Answer" type="xs:string" />
    </xs:all>
  </xs:complexType>

  <xs:simpleType name="NonEmptySTringType">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="GuidType">
    <xs:restriction base="xs:string">
      <xs:pattern value="^[a-z\d]{8}-[a-z\d]{4}-[a-z\d]{4}-[a-z\d]{4}-[a-z\d]{12}$" />
    </xs:restriction>
  </xs:simpleType>

